<!-- Form fields with fname, lname, username, admin and email for user-->

<%= f.label :First_name  %>
<%= f.text_field :fname, class: 'mb-1 form-control' %>

<%= f.label :Last_Name %>
<%= f.text_field :lname, class: 'mb-1 form-control' %>

<%= f.label :username %>
<%= f.text_field :username, class: 'mb-1 form-control' %>


<div class="mt-3 mb-3 custom-control custom-checkbox">
    <%= f.check_box :admin , class: 'mb-1  custom-control-input' %>
    <%= f.label :admin , class: 'custom-control-label' %>
</div>


<%= f.label :email %>
<%= f.email_field :email, class: 'mb-1 form-control' %>

